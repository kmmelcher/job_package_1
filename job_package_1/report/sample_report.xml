<odoo>
    <data>
        <!-- Report 1 -->
        <template id="report_sample_1_view">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.external_layout">
                        <span t-raw="o.get_contract_html_dynamic()"/>
                    </t>
                </t>
            </t>
        </template>

        <record id="report_sample_1" model="ir.actions.report">
            <field name="name">Contract</field>
            <field name="model">job.package.freelancer</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">job_package_1.report_sample_1_view</field>
            <field name="report_file">job_package_1.report_sample_1</field>
            <field name="binding_model_id" ref="model_job_package_freelancer"/>
            <field name="binding_type">report</field>
            <!-- Save report as attachment -->
            <field name="attachment">'Contract1.pdf'</field>
        </record>

